@model IEnumerable<WebClass.ViewModel.StudentVM>
@{
    ViewBag.Title = "GetStudent";
}

<div class="container" style="margin-top:50px">

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(c => c.Account)</th>
                <th>@Html.DisplayNameFor(c => c.Name)</th>
                <th>@Html.DisplayNameFor(c => c.BuyTime)</th>
                <th>@Html.DisplayNameFor(c => c.Gender)</th>
                <th>@Html.DisplayNameFor(c => c.Email)</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var class_ in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(c => class_.Account)</td>
                    <td>@Html.DisplayFor(c => class_.Name)</td>
                    <td>@Html.DisplayFor(c => class_.BuyTime)</td>
                    <td>@Html.DisplayFor(c => class_.Gender)</td>
                    <td>@Html.DisplayFor(c => class_.Email)</td>
                </tr>
            }
        </tbody>
    </table>
</div>